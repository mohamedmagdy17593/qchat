    "nodemon": "^2.0.15",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "sass": "^1.49.0",
    "tailwindcss": "^3.0.15",
    "ts-node": "^10.4.0",
    "typescript": "4.5.5"
