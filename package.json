    "@types/lodash": "^4.14.178",
    "@types/morgan": "^1.9.3",
    "@types/simple-peer": "^9.11.4",
    "axios": "^0.25.0",
    "clsx": "^1.1.1",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "iconoir": "^1.0.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nanoid": "^3.2.0",
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-toast": "^2.2.0",
    "react-icons": "^4.3.1",
    "simple-peer": "^9.11.0",
