{
  "name": "qchat",
  "private": true,
  "scripts": {
    "peer:server": "peerjs --port 9000 --key peerjs --path /peerjs",
    "dev": "nodemon --exec \"ts-node\" --project tsconfig.server.json server/index.ts",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "build": "npm run build:next && npm run build:server",
    "start": "NODE_ENV=production node dist/index.js",
    "lint": "next lint",
    "studio": "npx prisma studio"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.4.0",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "cors": "^2.8.5",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "morgan": "^1.10.0",
    "nanoid": "^3.2.0",
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "socket.io": "^4.4.1"
  },
  "devDependencies": {
    "@types/node": "17.0.10",
    "@types/react": "17.0.38",
    "autoprefixer": "^10.4.2",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.8",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "sass": "^1.49.0",
    "tailwindcss": "^3.0.15",
    "ts-node": "^10.4.0",
    "typescript": "4.5.5"
  }
}
